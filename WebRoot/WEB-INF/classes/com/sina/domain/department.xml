<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.1.7//EN"
	"http://struts.apache.org/dtds/struts-2.1.7.dtd">
<struts>
<package name="Department" namespace="/department" extends="struts-default" >
	<!-- class="EmployeeAction"指向spring 配置文件里的bean的id -->
	<action name="department*" class="DepartmentAction" method="{1}">
		<result name="ok">/WEB-INF/queryAll.jsp</result>
	</action>
</package>
</struts>    
